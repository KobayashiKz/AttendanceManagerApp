<layout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="android.view.View"/>

        <variable
                name="viewmodel"
                type="com.kk.attendancemanagerapp.attendancemanagerapp.basicsetting.BasicSettingViewModel"/>
    </data>


    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context="com.kk.attendancemanagerapp.attendancemanagerapp.basicsetting.BasicSettingFragment">

        <ListView
                android:id="@+id/basic_setting_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

    </FrameLayout>
</layout>
